{
  "Basic Package.json": {
    "prefix": "pkg",
    "body": [
      "{",
      "\t\"name\": \"${1:project-name}\",",
      "\t\"version\": \"${2:1.0.0}\",",
      "\t\"description\": \"${3:Project description}\",",
      "\t\"main\": \"${4:index.js}\",",
      "\t\"scripts\": {",
      "\t\t\"test\": \"${5:echo \\\"Error: no test specified\\\" && exit 1}\"",
      "\t},",
      "\t\"keywords\": [",
      "\t\t\"${6:keyword}\"",
      "\t],",
      "\t\"author\": \"${7:Your Name}\",",
      "\t\"license\": \"${8:ISC}\"",
      "}"
    ],
    "description": "Basic package.json template"
  },
  "React Package.json": {
    "prefix": "pkgreact",
    "body": [
      "{",
      "\t\"name\": \"${1:react-app}\",",
      "\t\"version\": \"0.1.0\",",
      "\t\"private\": true,",
      "\t\"dependencies\": {",
      "\t\t\"react\": \"^${2:18.2.0}\",",
      "\t\t\"react-dom\": \"^${2:18.2.0}\",",
      "\t\t\"react-scripts\": \"${3:5.0.1}\"",
      "\t},",
      "\t\"scripts\": {",
      "\t\t\"start\": \"react-scripts start\",",
      "\t\t\"build\": \"react-scripts build\",",
      "\t\t\"test\": \"react-scripts test\",",
      "\t\t\"eject\": \"react-scripts eject\"",
      "\t},",
      "\t\"eslintConfig\": {",
      "\t\t\"extends\": [",
      "\t\t\t\"react-app\"",
      "\t\t]",
      "\t},",
      "\t\"browserslist\": {",
      "\t\t\"production\": [",
      "\t\t\t\">0.2%\",",
      "\t\t\t\"not dead\",",
      "\t\t\t\"not op_mini all\"",
      "\t\t],",
      "\t\t\"development\": [",
      "\t\t\t\"last 1 chrome version\",",
      "\t\t\t\"last 1 firefox version\",",
      "\t\t\t\"last 1 safari version\"",
      "\t\t]",
      "\t}",
      "}"
    ],
    "description": "React package.json template"
  },
  "TypeScript Package.json": {
    "prefix": "pkgts",
    "body": [
      "{",
      "\t\"name\": \"${1:typescript-project}\",",
      "\t\"version\": \"${2:1.0.0}\",",
      "\t\"description\": \"${3:TypeScript project}\",",
      "\t\"main\": \"${4:dist/index.js}\",",
      "\t\"types\": \"${5:dist/index.d.ts}\",",
      "\t\"scripts\": {",
      "\t\t\"build\": \"tsc\",",
      "\t\t\"dev\": \"tsc --watch\",",
      "\t\t\"test\": \"jest\"",
      "\t},",
      "\t\"keywords\": [",
      "\t\t\"typescript\",",
      "\t\t\"${6:keyword}\"",
      "\t],",
      "\t\"author\": \"${7:Your Name}\",",
      "\t\"license\": \"${8:ISC}\",",
      "\t\"devDependencies\": {",
      "\t\t\"typescript\": \"^${9:5.0.0}\",",
      "\t\t\"@types/node\": \"^${10:18.0.0}\"",
      "\t}",
      "}"
    ],
    "description": "TypeScript package.json template"
  }
} 